{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "serviceBus": {
      "SessionHandlerOptions": {
        "MaxAutoRenewDuration": "00:01:00",
        "MessageWaitTimeout": "00:01:00",
        "MaxConcurrentSessions": 2,
        "AutoComplete": true
      }
    }
  }
}